<h1>Listing articles</h1>
<p><%= link_to 'New article', new_article_path %>
<table>
  <tr>
    <th>Id</th>
    <th>Title</th>
    <th>Actions</th>
  </tr>
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.id %></td>
      <td><%= article.title %></td>
      <td><%= link_to 'Show', article_path(article)%></td>
    </tr>
  <% end %>
</table>
